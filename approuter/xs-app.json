{
  "welcomeFile": "/bookshopdemoapp/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/api/(.*)$",
      "target": "/api/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2standalone/admin/(.*)$",
      "target": "/v2/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api_v2",
      "csrfProtection": true
    },
    {
      "source": "/v2standalone/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api_v2",
      "csrfProtection": true
    },
    {
      "source": "/v2/admin/(.*)$",
      "target": "/v2/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    }
  ]
}