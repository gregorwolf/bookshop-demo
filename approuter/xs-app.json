{
  "welcomeFile": "/bookshopdemoapp/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/fesr$",
      "target": "/fesr",
      "destination": "srv_api",
      "csrfProtection": false,
      "authenticationType": "xsuaa"
    },
    {
      "source": "/api/(.*)$",
      "target": "/api/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2standalone/(.*)$",
      "target": "/odata/v2/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api_v2",
      "csrfProtection": true
    },
    {
      "source": "/odata/v2/(.*)$",
      "target": "/odata/v2/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "^/basic/(.*)$",
      "target": "$1",
      "destination": "srv_api",
      "authenticationType": "basic",
      "csrfProtection": true
    },
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/public/(.*)$",
      "target": "/public/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    }
  ]
}
