{
  "_version": "1.15.0",
  "sap.app": {
    "id": "genres",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "ach": "CA-UI5-FE",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "offline": false,
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "ui5template.fiorielements.v4.lrop",
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resources": {
      "js": [],
      "css": []
    },
    "dependencies": {
      "minUI5Version": "",
      "libs": {
        "sap.fe.templates": {},
        "sap.ui.fl": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "GenresHierarchyList",
          "target": "GenresHierarchyList"
        },
        {
          "pattern": "GenresHierarchy({key}):?query:",
          "name": "GenresHierarchyObjectPage",
          "target": "GenresHierarchyObjectPage"
        }
      ],
      "targets": {
        "GenresHierarchyList": {
          "type": "Component",
          "id": "GenresHierarchyList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "GenresHierarchy",
              "initialLoad": "Enabled",
              "variantManagement": "Page",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "TreeTable",
                    "hierarchyQualifier": "HierarchyQ"
                  }
                }
              },
              "navigation": {
                "GenresHierarchy": {
                  "detail": {
                    "route": "GenresHierarchyObjectPage"
                  }
                }
              }
            }
          }
        },
        "GenresHierarchyObjectPage": {
          "type": "Component",
          "id": "GenresHierarchyObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "GenresHierarchy"
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.platform.abap": {
    "_version": "1.1.0",
    "uri": ""
  },
  "sap.platform.hcp": {
    "_version": "1.1.0",
    "uri": ""
  },
  "sap.fiori": {
    "_version": "1.1.0",
    "registrationIds": [],
    "archeType": "transactional"
  }
}
