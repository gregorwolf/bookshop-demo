{
    "_version": "1.37.0",
    "sap.app": {
        "id": "orders",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.6.7",
            "toolsId": "b0e13a96-fa7f-4101-89a1-874d0fde00ba"
        },
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/order/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.96.11",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ordersnew.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupId": "$direct"
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "orders.ext.ListReportExt"
                    },
                    "sap.fe.templates.ObjectPage.ObjectPageController": {
                        "controllerName": "orders.ext.ObjectPageExt"
                    }
                }
            }
        },
        "routing": {
            "config": {},
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "OrdersList",
                    "target": "OrdersList"
                },
                {
                    "pattern": "Orders({key}):?query:",
                    "name": "OrdersObjectPage",
                    "target": "OrdersObjectPage"
                },
                {
                    "pattern": "Orders({key})/Items({key2}):?query:",
                    "name": "OrderItemsObjectPage",
                    "target": "OrderItemsObjectPage"
                }
            ],
            "targets": {
                "OrdersList": {
                    "type": "Component",
                    "id": "OrdersList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Orders",
                            "variantManagement": "Page",
                            "navigation": {
                                "Orders": {
                                    "detail": {
                                        "route": "OrdersObjectPage"
                                    }
                                }
                            },
                            "quickVariantSelection": {
                                "variants": {
                                    "0": {
                                        "key": "New",
                                        "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#New"
                                    },
                                    "1": {
                                        "key": "InProcess",
                                        "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#InProcess"
                                    },
                                    "2": {
                                        "key": "Completed",
                                        "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Completed"
                                    },
                                    "3": {
                                        "key": "Empty",
                                        "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Empty"
                                    }
                                }
                            }
                        }
                    }
                },
                "OrdersObjectPage": {
                    "type": "Component",
                    "id": "OrdersObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Orders",
                            "navigation": {
                                "Items": {
                                    "detail": {
                                        "route": "OrderItemsObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "Comments/@com.sap.vocabularies.UI.v1.LineItem": {
                                    "columns": {
                                        "CommentHTML": {
                                            "header": "Comment",
                                            "template": "orders.ext.Comment",
                                            "width": "30%",
                                            "position": {
                                                "placement": "Before",
                                                "anchor": "DataField::createdAt"
                                            }
                                        }
                                    }
                            }
                        },
                            "content": {
                                "header": {
                                    "actions": {
                                        "addComment": {
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "Add Comment",
                                            "press": "orders.ext.ObjectPage.addComment"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "OrderItemsObjectPage": {
                    "type": "Component",
                    "id": "OrderItemsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "OrderItems"
                        }
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}
