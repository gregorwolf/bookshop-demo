{
  "_version": "1.15.0",
  "sap.app": {
    "id": "roleUI5latest",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "ach": "CA-UI5-FE",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/admin/",
        "type": "OData",
        "settings": {
          "annotations": ["annotation"],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      }
    },
    "offline": false,
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "ui5template.fiorielements.v4.lrop",
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resources": {
      "js": [],
      "css": []
    },
    "dependencies": {
      "minUI5Version": "",
      "libs": {
        "sap.fe.templates": {},
        "sap.ui.fl": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "extends": {
      "extensions": {
          "sap.ui.controllerExtensions": {
              "sap.fe.templates.ListReport.ListReportController": {
                  "controllerName": "roleUI5latest.ext.ListReportExt"
              }
          }
      }
  },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "RolesList",
          "target": "RolesList"
        },
        {
          "pattern": "Roles({key}):?query:",
          "name": "RolesObjectPage",
          "target": "RolesObjectPage"
        }
      ],
      "targets": {
        "RolesList": {
          "type": "Component",
          "id": "RolesList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Roles",
              "variantManagement": "Page",
              "initialLoad": "Enabled",
              "navigation": {
                "Roles": {
                  "detail": {
                    "route": "RolesObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "selectAll": true,
                    "enableMassEdit": true,
                    "beforeRebindTable": ".extension.roleUI5latest.ext.ListReportExt.addDragAndDrop"
                  }
                }
              }
            }
          }
        },
        "RolesObjectPage": {
          "type": "Component",
          "id": "RolesObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Roles",
              "navigation": {},
              "controlConfiguration": {
                "Assigned Business Objects": {
                  "tableSettings": {
                    "creationMode": {
                      "name": "Inline"
                    }
                  }
                },
                "Assigned Users": {
                  "tableSettings": {
                    "creationMode": {
                      "name": "Inline"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.platform.abap": {
    "_version": "1.1.0",
    "uri": ""
  },
  "sap.platform.hcp": {
    "_version": "1.1.0",
    "uri": ""
  },
  "sap.fiori": {
    "_version": "1.1.0",
    "registrationIds": [],
    "archeType": "transactional"
  }
}
