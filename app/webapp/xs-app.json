{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/xsuaa/(.*)$",
      "target": "$1",
      "destination": "bookshop-demo",
      "authenticationType": "ias",
      "csrfProtection": true
    },
    {
      "source": "^/basic/(.*)$",
      "target": "$1",
      "destination": "bookshop-demo",
      "authenticationType": "basic",
      "csrfProtection": true
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "ias"
    }
  ]
}
